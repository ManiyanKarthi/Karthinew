{
  "name": "loopback4",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t loopback4 .",
    "docker:run": "docker run -p 3000:3000 -d loopback4",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "backbuild": "rimraf build1 && cross-env NODE_ENV=production webpack --config webpack.js",
    "backbuild1": "rimraf build1 && cross-env NODE_ENV=production webpack --config webpack.config.babel.backend.js",
    "prodbuild": "rimraf build1 && cross-env NODE_ENV=production webpack --config webpack-loopback.js",
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.2.4",
    "@loopback/context": "^1.14.0",
    "@loopback/core": "^1.6.1",
    "@loopback/example-log-extension": "^1.1.15",
    "@loopback/openapi-v3": "^1.3.8",
    "@loopback/repository": "^1.5.2",
    "@loopback/rest": "^1.10.5",
    "@loopback/rest-explorer": "^1.1.19",
    "@loopback/service-proxy": "^1.1.7",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-request": "^1.4.0",
    "cldr": "^5.2.0",
    "compact-json-loader": "^0.1.1",
    "cross-env": "^5.2.0",
    "eslint-loader": "^2.1.2",
    "globalize": "^1.4.2",
    "gulp": "^4.0.2",
    "log4js": "^4.1.1",
    "loopback": "^3.25.1",
    "loopback-boot": "^2.28.0",
    "loopback-component-logger": "^1.0.0",
    "loopback-webpack-plugin": "^0.1.2",
    "nexe": "^3.2.0",
    "pkg": "^4.4.0",
    "pm2": "^3.5.0",
    "raw-loader": "^2.0.0",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.12",
    "strong-globalize": "^4.1.3",
    "tsc": "^1.20150623.0",
    "typescript-compiler": "^1.4.1-2",
    "uglifyjs": "^2.4.11",
    "uniqid": "^5.0.3",
    "uws": "^100.0.1",
    "webpack": "^4.31.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@loopback/build": "^1.5.1",
    "@loopback/testlab": "^1.2.6",
    "@loopback/tslint-config": "^2.0.4",
    "@types/node": "^10.11.2",
    "eslint": "^5.16.0",
    "json-loader": "^0.5.7",
    "ts-loader": "^6.0.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.5",
    "webpack-cli": "^3.3.2"
  }
}
